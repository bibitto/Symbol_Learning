{
  "parser": "@typescript-eslint/parser",
  "root": true,
  "parserOptions": {
    "sourceType": "module",
    "project": "./tsconfig.json",
    "tsconfigRootDir": "./"
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": { "no-unused-vars": "off", "@typescript-eslint/no-unused-vars": ["off"], "@typescript-eslint/no-var-requires": ["off"] }
}
